{
  "name": "softark/yii2-cropper-bs5",
  "description": "Yii2 Bootstrap Cropper Input Widget for bootstrap 5",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","cropper","widget"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ercan Bilgin",
      "email": "bilginnet@gmail.com",
      "homepage": "https://github.com/bilginnet?tab=repositories"
    },
    {
      "name": "Nobuo Kihara",
      "email": "softark@gmail.com",
      "homepage": "https://github.com/softark?tab=repositories"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,  
  "require": {
    "php": ">=7.0",
    "bower-asset/cropperjs": "~1.5",
    "bower-asset/jquery-cropper": "~1.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap5": "*"
  },
  "autoload": {
    "psr-4": {
      "softark\\cropper\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }  
}
